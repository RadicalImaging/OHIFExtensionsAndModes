{
  "name": "@radicalimaging/extension-dicom-html",
  "version": "3.15.0",
  "description": "OHIF extension for rendering structured reports to HTML",
  "author": "Michel Dubois",
  "license": "MIT",
  "repository": "OHIF/Viewers",
  "main": "dist/@radicalimaging/extension-dicom-html/index.umd.js",
  "module": "src/index.tsx",
  "engines": {
    "node": ">=14",
    "npm": ">=6",
    "yarn": ">=1.18.0"
  },
  "files": [
    "public/**",
    "dist/**",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config .webpack/webpack.prod.js",
    "build:package": "yarn run build",
    "publishItem": "yarn publish",
    "start": "yarn run dev",
    "test:unit": "jest --watchAll",
    "test:unit:ci": "jest --ci --runInBand --collectCoverage --passWithNoTests"
  },
  "peerDependencies": {
    "@ohif/core": "^3.0.0",
    "@ohif/ui": "^2.0.0",
    "@radicalimaging/config-mode": ">=3.16.0",
    "dcmjs": "^0.29.6",
    "dicom-parser": "^1.8.9",
    "hammerjs": "^2.0.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "7.17.9"
  }
}
